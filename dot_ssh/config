Host *
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h-%p
  ControlPersist 600
  UseKeychain yes
  AddKeysToAgent yes

Host *
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"

# ~.ssh/config file being modified by script from Jamf for okta ASA configuration.
Match exec "/usr/local/bin/sft resolve -q %h"
ProxyCommand "/usr/local/bin/sft" proxycommand %h
UserKnownHostsFile "/Users/cwoodcox/Library/Application Support/ScaleFT/proxycommand_known_hosts"

